{
	"builds": [{
		"src": "pythoncharts/wsgi.py",
		"use": "@vercel/python",
		"config": {"maxLambdaSize": "15mb","maxDuration": 60, "runtime": "python3.10"}
	}],
	"routes": [
		{
			"src": "/(.*)",
			"dest": "pythoncharts/wsgi.py"
		}]
	"functions": {
    "api/**/*.py": {
      "excludeFiles": "public/**"
    }
  }
}
